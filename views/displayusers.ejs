<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <script src="https://kit.fontawesome.com/b4b1828725.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="/public/css/displayusers.css">
    <title>Document</title>
</head>

<body>
    <div class="header">
        <nav>
            <a href="/adminLoginLanding"><img src="/public/media/1.png" class="logo"></a>
            <h2 style="color:white;">USER DATA</h2>
            <button type="submit" class="login"><a href="/logout">LOGOUT</a></button>
        </nav>
    </div>
    <div class="banner">
        <!-- <h1>USER DATA</h1><br> -->
        <table class="table table-hover" id="jsonTable">
            <!-- <h1>USER DATA</h1> -->
            <thead class="thead-dark">
                <tr>
                    <th scope="col">User ID</th>
                    <th scope="col">NAME</th>
                    <th scope="col">EMAIL</th>
                    <th scope="col">PHONE NO</th>
                    <!-- <th scope="col">DELETE</th> -->
                </tr>
            </thead>
            <tbody>

                <% for(var i=0; i < data.length; i++) { %>
                    <tr>
                        <td>
                            <%= data[i].id %>
                        </td>
                        <td>
                            <%= data[i].name %>
                        </td>
                        <td>
                            <%= data[i].email %>
                        </td>
                        <td>
                            <%= data[i].phoneNo %>
                        </td>
                        <!-- <td>
                            <a href="<%= '/bookedPage/?carNo='+data[i].carNo %>">
                                Delete
                            </a>
                        </td> -->
                    </tr>
                    <% } %>


            </tbody>
        </table>

    </div>
    <script src="https://kit.fontawesome.com/b4b1828725.js" crossorigin="anonymous"></script>
</body>

</html>